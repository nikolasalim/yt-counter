{
  "manifest_version": 2,
  "name": "YT Counter",
  "version": "1.0.0",
  "default_locale": "en",
  "description": "An extention to count how many minutes you've watched on YouTube",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
